package com.fdmgroup.dynamicwebwalkthrough.moviemanagement;

import java.util.ArrayList;
import java.util.List;

public class MovieController {
	
	public List<Movie> getAllMovie() {
		MovieFactory movieFactory = new MovieFactory();
		List<Movie> all = new ArrayList<Movie>();
		all.add(movieFactory.createMovie("Star War", 10.00));
		all.add(movieFactory.createMovie("Staar", 14.00));
		all.add(movieFactory.createMovie("St War", 1440.56));
		all.add(movieFactory.createMovie("Starar", 130.00));
		return all;
	}
}
